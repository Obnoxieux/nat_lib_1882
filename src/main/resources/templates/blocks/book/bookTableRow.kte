@import dev.obnx.model.Book
@param book: Book

<tr>
    <td>${book.id}</td>
    <td>${book.number}</td>
    <td dir="rtl">${book.title}</td>
    <td dir="rtl">${book.author}</td>
    <td dir="rtl">ENDOWMENT</td>

    @if(book.manuscript)
        <td>✅</td>
    @else
        <td>❌</td>
    @endif

    @if(book.print)
        <td>✅</td>
    @else
        <td>❌</td>
    @endif

    <td>${book.volume}</td>
    <td dir="rtl">${book.comment}</td>
    <td>${book.editorComment}</td>
</tr>